C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;QAChC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;IACxB;IACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACT,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACnB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACrB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;IACvB","file":"/Users/ericaaron/Developer/Git/rocket/src/views/Search.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"search-main\">\n        <div id=\"bodyBox\" @click=\"bodyClick()\">\n            <video id=\"liveBgBox\" autoplay=\"true\" loop=\"true\" muted=\"true\"></video>\n            <img class=\"bgbox\" id=\"bgbox\" :class=\"{'focus':boxFocus, 'focus':courseFocus}\" alt=\"\" src=\"../assets/images/search-bg.jpg\" style=\"display: block; opacity: 1;\">\n            <div class=\"cover\" id=\"cover\" style=\"opacity: 1;\"></div>\n            <div class=\"title\" id=\"title\" :class=\"{'title-move':boxFocus, 'none':courseFocus}\">\n                <h1 id=\"titleText\">12:44</h1>\n            </div>\n            <div id=\"searchOptBox\" class=\"\">\n                <span class=\"searchOpt selected\" id=\"baiduSearchOpt\">\n                    <i class=\"iconfont\" id=\"iconBaidu\">Baidu</i>\n                </span>\n                <span class=\"searchOpt\" id=\"bingSearchOpt\">\n                    <i class=\"iconfont\" id=\"iconBing\">Bing</i>\n                </span>\n                <span class=\"searchOpt\" id=\"googleSearchOpt\">\n                    <i class=\"iconfont\" id=\"iconGoogle\">Google</i>\n                </span>\n            </div>\n            <input type=\"text\" class=\"input ypoctonod\" @click=\"inputClick()\" :class=\"{'none':courseFocus}\" id=\"input0\" name=\"word\" size=\"30\" placeholder=\"Search\" autocomplete=\"off\" style=\"opacity: 1;\">\n            <div class=\"keyword\" id=\"keyword\" style=\"height: auto; opacity: 0; display: none;\"></div>\n            <div class=\"quotebox waves-effect waves-light hide\" id=\"quotebox\" style=\"z-index: 1; animation: 0s ease 0s 1 normal none running none;\">\n                <span class=\"quote-bg\"></span>\n                <p class=\"quote-content\" id=\"quoteContent\"></p>\n                <p class=\"quote-author\" id=\"quoteAuthor\"></p>\n                <span id=\"btnAddQuoteToNote\" role=\"button\" tabindex=\"0\"><i class=\"iconfont\" id=\"iconAddQuoteToNote\"></i><span id=\"txtAddQuoteToNote\"> 收藏到便笺</span></span>\n            </div>\n            <div class=\"course\" :class=\"{'course-click':courseStatus}\"  @click=\"courseClick()\">\n                <h1>{{course.name}}</h1>\n                <h1>{{course.teacher}}</h1>\n                <h1>{{course.classroom}}</h1>\n                <h1>{{course.time}}</h1>\n            </div>\n\n        </div>\n    </div>\n    <nav-buttom></nav-buttom>\n</template>\n\n<script>\n    import NavButtom from \"../components/NavButtom\";\n    import axios from \"axios\";\n    export default {\n        name: \"Search\",\n        components: {NavButtom},\n        data(){\n            return{\n                boxFocus: false,\n                courseFocus: false,\n                focus: false,\n                courseStatus:false,\n                url: '/API/Class/CourseJson/Louis/',\n                courseList: [],\n                course: {\n                    name: '',\n                    teacher: '',\n                    classroom: '',\n                    time: '',\n                },\n                timeInfo: [\n                    \"8:00 - 9:40\",\n                    \"8:00 - 9:40\",\n                    \"10:00 - 11:40\",\n                    \"10:00 - 11:40\",\n                    \"13:30 - 15:10\",\n                    \"13:30 - 15:10\",\n                    \"16:30 - 18:30\",\n                    \"16:30 - 18:30\"\n                ],\n            }\n        },\n        mounted() {\n            const date = new Date()\n            const week = date.getDay()\n            const hour = date.getHours()\n            const min = date.getMinutes()\n            let which = this.whichCourse(hour, min);\n            // console.log(week, hour, min);\n            axios.get(this.url).then(res => {\n                console.log(res.data);\n                let Res  = res.data\n                let courseInfo = res.data[week-1].节次\n                // console.log(this.course);\n\n                /*\n                for (let i = 0; i < this.course.length; i++){\n                    console.log(this.course[i]);\n                }\n                */\n                // which = 3;\n                let course = null;\n                const result = courseInfo.filter(function (item, index, array) {\n                    // console.log(item, index)\n                    if (parseInt(item.节次名) === which) {\n                        course = item.课程[0];\n                    }\n                });\n                if (course == null && week !== 7){\n                    course = Res[week].节次[0].课程[0]\n                    which = parseInt(Res[week].节次[0].节次名)\n                    Res = null;\n                }\n                this.courseList = course;\n                course = null;\n                this.course.name = this.courseList.课程名\n                this.course.teacher = this.courseList.教师\n                this.course.classroom = this.courseList.教室\n                this.course.time = this.timeInfo[which]\n                console.log(this.courseList);\n                console.log(this.course);\n            })\n\n            // console.log(typeof this.course);\n\n\n\n        },\n        methods:{\n            whichCourse(hour, min=0){\n                if (hour >= 0 && hour < 8){\n                    return 1;\n                }\n                else if(hour >=8 && hour <10) {\n                    return 3;\n                }\n                else if(hour >=10 && hour <14) {\n                    return 5;\n                }\n                else if(hour >=14 && hour <16) {\n                    return 7;\n                }\n                else if(hour >=19 && hour <24) {\n                    return 0;\n                }\n            },\n            bodyClick() {\n                if (this.focus === true){\n                    this.boxFocus = false;\n                    this.focus = false;\n                }\n            },\n            inputClick() {\n                this.boxFocus =  true;\n                const that = this;\n                setTimeout(function (){\n                    that.focus = true;\n                }, 100);\n            },\n            courseClick() {\n                this.courseFocus =  !this.courseFocus;\n                this.courseStatus = !this.courseStatus;\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    @import \"../assets/css/search-main.css\";\n\n    @media screen and (max-width: 600px){\n        .title-move {\n            position: absolute;\n            margin-top: 10vh;\n            width: 80%;\n            right: 15%;\n            font-size: 10vw;\n        }\n        .input:hover{\n            left: 10%;\n            top: 30vh;\n            width: 80%;\n        }\n        .course{\n            width: 90% !important;\n        }\n    }\n/*    .search-main{\n        position: absolute;\n        height: 100vh;\n        width: 100vw;\n        display: flex;\n        justify-content: center;\n        justify-items: center;\n    }*/\n    .none{\n        display: none;\n        opacity: 0;\n    }\n    .course{\n        margin: 0 auto;\n        opacity: 0.5;\n        position: relative;\n        top:85%;\n        background-color: white;\n        width: 50%;\n        height: 50%;\n        border-radius: 5vw;\n        color: ;\n        transition: all 0.25s;\n        transition-property: all;\n        transition-duration: 0.25s;\n        transition-timing-function: ease;\n        transition-delay: 0s;\n    }\n    .course-click{\n        top: 20% !important;\n        width: 80% !important;\n        border-radius: 10vw;\n    }\n\n</style>\n"]}]}