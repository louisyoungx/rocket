AAM,EAAC;AARH,KAAN;AAUD,GAdY;;AAeb,EAAA,OAAM,GAAK;AACT,IAAA,KAAK,CAAC,GAAN,CAAU,KAAK,GAAf,EAAoB,IAApB,CAAyB,GAAG,IAAE;AAC5B,MAAA,OAAO,CAAC,GAAR,CAAY,GAAG,CAAC,IAAhB;AACA,WAAK,GAAL,GAAW,GAAG,CAAC,IAAf;AACA,WAAK,KAAL,GAAa,IAAb;AACA,WAAK,KAAL,GAAa,SAAb;AAEA,WAAK,QAAL,CAAc,MAAI;AAChB,YAAI,OAAJ,CAAY,UAAZ,EAAwB;AACtB,UAAA,UAAU,EAAE,IADU;AAEtB,UAAA,SAAS,EAAE,IAFW;AAGtB,UAAA,eAAe,EAAE,IAHK,CAItB;;AAJsB,SAAxB;AAMD,OAPD;AAQD,KAdD;AAeD,GA/BY;;AAgCb,EAAA,OAAO,EAAC;AACN,IAAA,SAAS,GAAG;AACV,UAAG,KAAK,KAAL,KAAe,IAAlB,EAAwB;AACtB,aAAK,OAAL,GAAe,IAAf;AACA,cAAM,IAAG,GAAI,IAAb;AACA,QAAA,UAAU,CAAC,YAAY;AACrB,UAAA,IAAI,CAAC,OAAL,GAAe,KAAf;AACA,UAAA,IAAI,CAAC,MAAL,GAAc,EAAd;AACA,UAAA,IAAI,CAAC,MAAL,GAAc,IAAd;AACA,UAAA,IAAI,CAAC,IAAL,GAAY,MAAZ;AACA,UAAA,UAAU,CAAC,YAAY;AACrB,YAAA,IAAI,CAAC,MAAL,GAAc,KAAd;AACA,YAAA,IAAI,CAAC,IAAL,GAAY,MAAZ;AACA,YAAA,IAAI,CAAC,MAAL,GAAc,EAAd;AACD,WAJS,EAIP,IAJO,CAAV;AAKD,SAVS,EAUP,GAVO,CAAV;AAWF;AACD;;AAjBK;AAhCK,CAAf","sourcesContent":["<template>\n  <div class=\"all wrapper\">\n    <loading-rocket :ShowTitle=\"title\" :style=\"{display: load}\" :class=\"{'rocket-fadeOut':fadeOut}\" @click=\"isLoading()\"></loading-rocket>\n    <div id=\"app\" class=\"container class-container main\" :style=\"{display: loaded}\" :class=\"{'rocket-fadeIn':fadeIn}\">\n      <h2 class=\"class-title\">180851班青年大学习</h2>\n      <!--总结-->\n      <div>\n        <p>180851班共 {{res.AllNum}} 人，已完成 {{res.DoNum}} 人，未完成 {{res.DontNum}} 人</p>\n      </div>\n      <div class=\"class-card-container\">\n        <!--已完成名单-->\n        <div class=\"card class-card\" style=\"width: 18rem;\">\n          <div class=\"card-body\">\n            <h5 class=\"card-title\">已完成</h5>\n            <h6 class=\"card-subtitle mb-2 text-muted\">{{res.Time}}</h6>\n            <div v-for=\"member in res.DoMember\">\n              <p>{{member.index}}-{{member.name}}</p>\n            </div>\n          </div>\n        </div>\n        <!--未完成名单-->\n        <div  class=\"card class-card\" style=\"width: 18rem;\">\n          <div class=\"card-body\">\n            <h5 class=\"card-title\">未完成</h5>\n            <h6 class=\"card-subtitle mb-2 text-muted\">{{res.Time}}</h6>\n            <div v-for=\"member in res.NotMember\">\n              <p>{{member.index}}-{{member.name}}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n  import axios from \"axios\";\n  import $ from 'jquery'\n  import 'bootstrap/dist/css/bootstrap.min.css'\n  import 'bootstrap/dist/js/bootstrap.min'\n  import BScroll from 'better-scroll'\n  import LoadingRocket from '../components/LoadingRocket'\n  import Navbar from '../components/Navbar'\n\n  export default {\n    name: 'DxxInfo',\n    components: { Navbar, LoadingRocket },\n    data(){\n      return{\n        title:\"L o a d i n g . . . \",\n        enter:false,\n        url:\"/API/Class/DXXJson/\",\n        res:\"\",\n        load:'',\n        loaded:'none',\n        fadeOut:false,\n        fadeIn:false,\n      }\n    },\n    mounted () {\n      axios.get(this.url).then(res=>{\n        console.log(res.data)\n        this.res = res.data\n        this.enter = true\n        this.title = \"点 击 进 入\"\n\n        this.nextTick(()=>{\n          new BScroll('.wrapper', {\n            pullUpLoad: true,\n            scrollbar: true,\n            pullDownRefresh: true\n            // and so on\n          })\n        })\n      })\n    },\n    methods:{\n      isLoading() {\n        if(this.enter === true) {\n          this.fadeOut = true;\n          const that = this\n          setTimeout(function () {\n            that.fadeOut = false;\n            that.loaded = '';\n            that.fadeIn = true;\n            that.load = 'none';\n            setTimeout(function () {\n              that.fadeIn = false;\n              that.load = 'none';\n              that.loaded = '';\n            }, 1000)\n          }, 300)\n        }\n      },\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .all {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    text-align: center;\n    color: #2c3e50;\n  }\n  body{\n    background: #fff;\n  }\n  .class-container{\n    margin-top: 10%;\n  }\n  .class-title{\n    margin-bottom: 5%;\n  }\n  .class-card-container{\n    display: flex;\n    flex-wrap: wrap;\n    justify-items: center;\n    justify-content: space-around;\n    align-items: flex-start;\n    align-content: space-around;\n  }\n  .class-card{\n    margin-bottom: 5%;\n  }\n\n</style>\n"],"sourceRoot":""}]}